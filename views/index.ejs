<!DOCTYPE html>

<head>
  <meta charset="utf-8">
  <title>LeanEngine websocket demo</title>
  <style>
    body {
      margin: 0;
      padding: 0;
    }

    #term {
      position: absolute;
      margin: 0;
      padding: 0;
      width: 100%;
      height: 100%;
    }
  </style>
</head>

<body>
  <div id="term">

  </div>

  <script src="../jquery-1.7.2.min.js"></script>
  <script>
    jQuery(function ($, undefined) {
      var url = 'ws://' + window.location.host + '/open/echo';
      var connection = null;
      connection = new WebSocket(url);
      connection.onopen = function () {
        console.log('connected!');
        var json={objid:"<%=objid%>",door:"001"};
        console.log(json);
        connection.send("001");
      };
      connection.onerror = function (err) {
        console.log('error: ' + err);
      };
      connection.onmessage = function (e) {
        console.log('< ' + e.data);
      };
      connection.onclose = function (e) {
        console.log('connection closed!');
        connection = null;
      }
    });
  </script>
</body>
